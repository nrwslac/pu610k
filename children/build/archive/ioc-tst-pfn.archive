# iocAdmin Archive template
# Format:
# <PVNAME> <PERIOD> [ monitor ]

# Archive these values only when they change
IOC:TST:PFN:APP_DIR1 30 monitor 
IOC:TST:PFN:APP_DIR2 30 monitor 
IOC:TST:PFN:BSP_REV 30 monitor
IOC:TST:PFN:CA_CLNT_CNT 1 monitor 
IOC:TST:PFN:CA_CONN_CNT 1 monitor 
IOC:TST:PFN:EPICS_VERS 30 monitor
IOC:TST:PFN:FD_CNT 1 monitor 
IOC:TST:PFN:FD_FREE 1 monitor 
IOC:TST:PFN:FD_MAX 1 monitor 
IOC:TST:PFN:HOSTNAME 30 monitor
IOC:TST:PFN:KERNEL_VERS 30 monitor
IOC:TST:PFN:RECORD_CNT 10 monitor
IOC:TST:PFN:STARTTOD 30 monitor
IOC:TST:PFN:ST_SCRIPT1 30 monitor 
IOC:TST:PFN:ST_SCRIPT2 30 monitor 
IOC:TST:PFN:SUSP_TASK_CNT 30 monitor 
IOC:TST:PFN:SYSRESET 30 monitor 

# Archive these values at the specified rate
IOC:TST:PFN:DAT_MBUF_FREE 10
IOC:TST:PFN:DAT_MBUF_MAX 10
IOC:TST:PFN:LOAD 30 
IOC:TST:PFN:MEM_BLK_FREE 30 
IOC:TST:PFN:MEM_FREE 30 
IOC:TST:PFN:MEM_USED 30 
IOC:TST:PFN:SYS_MBUF_FREE 30 
